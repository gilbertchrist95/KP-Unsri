<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/home"
        android:id="@+id/home"
        android:background="@android:color/transparent"
        android:layout_marginTop="35dp"
        android:layout_marginRight="39dp"
        android:layout_marginEnd="39dp"
        android:layout_alignParentTop="true"
        android:layout_toLeftOf="@+id/movie"
        android:layout_toStartOf="@+id/movie" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="MOVIE"
        android:id="@+id/movie"
        android:layout_alignBottom="@+id/home"
        android:layout_centerHorizontal="true" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/list"
        android:id="@+id/list"
        android:background="@android:color/transparent"
        android:layout_marginLeft="37dp"
        android:layout_marginStart="37dp"
        android:layout_alignBottom="@+id/movie"
        android:layout_toRightOf="@+id/movie"
        android:layout_toEndOf="@+id/movie" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/icon1"
        android:id="@+id/icon1"
        android:background="@android:color/transparent"
        android:layout_below="@+id/home"
        android:layout_alignLeft="@+id/home"
        android:layout_alignStart="@+id/home"
        android:layout_marginTop="39dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Icon 1"
        android:id="@+id/icon1teks"
        android:layout_below="@+id/icon1"
        android:layout_alignLeft="@+id/icon1"
        android:layout_alignStart="@+id/icon1" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/icon2"
        android:id="@+id/imageButton"
        android:background="@android:color/transparent"
        android:layout_above="@+id/icon1teks"
        android:layout_centerHorizontal="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Icon 2"
        android:id="@+id/icon2teks"
        android:layout_below="@+id/imageButton"
        android:layout_alignLeft="@+id/imageButton"
        android:layout_alignStart="@+id/imageButton" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/icon3"
        android:id="@+id/icon3"
        android:background="@android:color/transparent"
        android:layout_alignTop="@+id/imageButton"
        android:layout_alignLeft="@+id/list"
        android:layout_alignStart="@+id/list" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Icon 3"
        android:id="@+id/icon3teks"
        android:layout_below="@+id/icon3"
        android:layout_alignLeft="@+id/icon3"
        android:layout_alignStart="@+id/icon3" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/icon4"
        android:id="@+id/icon4"
        android:background="@android:color/transparent"
        android:layout_centerVertical="true"
        android:layout_alignLeft="@+id/icon1teks"
        android:layout_alignStart="@+id/icon1teks" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Icon 4"
        android:id="@+id/icon4teks"
        android:layout_below="@+id/icon4"
        android:layout_alignLeft="@+id/icon4"
        android:layout_alignStart="@+id/icon4" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/icon5"
        android:id="@+id/icon5"
        android:background="@android:color/transparent"
        android:layout_alignTop="@+id/icon4"
        android:layout_alignLeft="@+id/icon2teks"
        android:layout_alignStart="@+id/icon2teks" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Icon 5"
        android:id="@+id/icon5teks"
        android:layout_alignTop="@+id/icon4teks"
        android:layout_alignLeft="@+id/icon5"
        android:layout_alignStart="@+id/icon5" />


</RelativeLayout>